{
  "name": "TwTv-farm",
  "description": "Chrome Extension boilerplate created with React Typescript.",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "matches": ["https://www.twitch.tv/*"],
    "exclude_matches": ["https://www.twitch.tv/videos/*"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "exclude_matches": ["https://www.twitch.tv/videos/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "notifications", "http://*/*", "https://*/*"]
}
